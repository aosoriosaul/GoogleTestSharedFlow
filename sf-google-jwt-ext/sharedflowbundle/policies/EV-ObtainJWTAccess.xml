<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ExtractVariables continueOnError="false" enabled="true" name="EV-ObtainJWTAccess">
    <DisplayName>EV-ObtainJWTAccess</DisplayName>
    <Properties/>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <Source clearPayload="false">jwtResponse</Source>
    <Header name="jwt-token">
        <Pattern>{jwtResponse.header.jwt-token}</Pattern>
    </Header>
    <!--<Source clearPayload="false">tokenResponse</Source>-->
    <VariablePrefix>apigee</VariablePrefix>
</ExtractVariables>